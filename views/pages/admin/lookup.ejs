<%- include("../partials/header", { title: "Student Lookup"}) %>
    <form method="POST">
        <div class="field">
            <label class="label">Name</label>
            <div class="control has-icons-left has-icons-right">
                <input class="input" type="name" name="name" placeholder="Search via name">
                <span class="icon is-small is-left"><i class="fas fa-user"></i></span>
            </div>
        </div>
        <div class="field is-grouped">
            <div class="control">
                <button type="submit" class="button is-link">Continue</button>
            </div>
        </div>
    </form>
    <br>
    <small>If you are a student and caught using a administrative account, punishment will be given.</small>
    <%- include("../partials/footer") %>